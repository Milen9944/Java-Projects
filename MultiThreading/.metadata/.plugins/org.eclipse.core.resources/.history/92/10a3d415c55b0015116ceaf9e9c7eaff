package NoThreadPool;

public class Beginning {

	public static void main(String[] args) {
		Thread t1 = (Thread) new FirstThread();
		Thread t2 = new SecondThrea();
		Thread t3 = new ThirdThread();
		Thread t4 = new FourthThread();
		Thread t5 = new FifthThread();
		t1.setName("Claudia");
		t2.setName("Lidia");
		t3.setName("Glori");
		t4.setName("Viki");
		t5.setName("Alex");
		t1.start();
		t2.start();
		t3.start();
		t4.start();
		t5.start();
	}

}
