
public class Cat {

	private String name;
	private int age;
	
	public Cat(){
		
		this.name = "";
		this.age = 0;
	}
	public Cat(String name , int age){
		this.name = getName();
		this.age = getAge();
		
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	
	public void Say(){
		
		System.out.println("My name is: " + this.name + "and I'm " 
		+ this.age + " years old");
		System.out.println();
			
	}
}
