package Arrays;

public class MultiplyArrayElementsBy10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] anArray = new int[10];
		
		for(int i = 0 ; i < anArray.length ; i++){
			
			anArray[i] = i * 10;
			System.out.println(anArray[i] + " ");
			
		}
		
		
	}

}
