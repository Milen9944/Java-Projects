package Arrays;

import java.util.Scanner;
import java.util.Random;

public class BinarySearch {

	public static void main(String[] args) {

		Random randomizator = new Random();
		Scanner scan = new Scanner(System.in);
		
		System.out.println("Input the number of elements");
		int element = scan.nextInt();
		int[] searchArray = new int[element];
		
		System.out.println("Input the target");
		int target = scan.nextInt();
		
		int middle = (searchArray.length/2);
		System.out.println(middle);
		int temp = 0;
		
		
		for(int i = 0; i < (searchArray.length - 1) ; i++){
			
			//searchArray[i] = randomizator.nextInt(searchArray.length);
			if(searchArray[i] > searchArray[i+1]){
				searchArray[i] = searchArray[i] + searchArray[i+1];
				searchArray[i+1] = searchArray[i] - searchArray[i+1];
				searchArray[i] = searchArray[i] - searchArray[i+1];
			
		  }else if(searchArray[i] < searchArray[i+1]){
				
				continue;
				
			}
			System.out.println(i);
		}
		
	}

}
