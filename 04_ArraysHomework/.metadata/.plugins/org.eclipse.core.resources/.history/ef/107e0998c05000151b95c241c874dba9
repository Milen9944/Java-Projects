package Arrays;

public class PrintingNumsFromThirtyToOneInFiveRowsAndSixColumns {

	public static void main(String[] args) {
		
		int[][] num = new int[5][6];
		int material = 30;
	
		
		for(int i = 0; i < num.length; i++){
			
			System.out.println();
			
			for(int j = 0 ; j < num[i].length; j++){
				num[i][j] = material;
				System.out.println(num[i][j] + "\t" );
				material --;
				//System.out.println(num[i][j] + "\t" );
			}
			
		}
		
	}

}
