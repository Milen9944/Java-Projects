package Arrays;

public class BubbleSort {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] arrayForSorting = {5 , 9 , 12 , 16 , 18 , 24};
		
		for(int i = 0; i < arrayForSorting.length  ; i++){
			for(int j = 1; j < arrayForSorting.length - 1  ; j++)
			if(arrayForSorting[i] < arrayForSorting[i+1]){
				arrayForSorting[i] = arrayForSorting[i] + arrayForSorting[i+1];
				arrayForSorting[i + 1] =  arrayForSorting[i] - arrayForSorting[i+1];
				arrayForSorting[i] = arrayForSorting[i] - arrayForSorting[i+1];
				
			}
			System.out.println(arrayForSorting[i]);
		}
		
	}

}
